{
  "name": "@particular-tools/symbols",
  "version": "1.0.0",
  "description": "A collection of JS symbols and utilities to define common semantics",
  
  "main": "dist/index.js",
  "typings": "dist/index.d.ts",
  "module": "dist/lib.esm.js",
  "files": [
    "dist",
    "src"
  ],
  "engines": {
    "node": ">=10"
  },
  "scripts": {
    "start": "tsdx watch",
    "build": "tsdx build",
    "test": "tsdx test",
    "lint": "tsdx lint",
    "prepare": "tsdx build",
    "size": "size-limit",
    "analyze": "size-limit --why"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/particular-tools/js-symbols.git"
  },
  "author": "Tal Shani",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/particular-tools/js-symbols/issues"
  },
  "homepage": "https://github.com/particular-tools/js-symbols#readme",
  "husky": {
    "hooks": {
      "pre-commit": "tsdx lint"
    }
  },
  "size-limit": [
    {
      "path": "dist/lib.cjs.production.min.js",
      "limit": "10 KB"
    },
    {
      "path": "dist/lib.esm.js",
      "limit": "10 KB"
    }
  ],
  "devDependencies": {
    "@size-limit/preset-small-lib": "^8.0.1",
    "husky": "^8.0.1",
    "size-limit": "^8.0.1",
    "tsdx": "^0.14.1",
    "tslib": "^2.4.0",
    "typescript": "^4.7.4"
  }
}
